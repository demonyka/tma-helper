{
    "name": "tma-helper",
    "version": "0.0.1",
    "description": "Telegram Mini Apps helper library for Vue 3 projects",
    "main": "dist/index.cjs.js",
    "module": "dist/index.es.js",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.es.js",
            "require": "./dist/index.cjs.js"
        }
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "vite build",
        "dev": "vite build --watch",
        "prepare": "npm run build"
    },
    "keywords": [
        "telegram",
        "miniapp",
        "vue3",
        "tma",
        "helper",
        "sdk"
    ],
    "author": "Dmitrii Isaev",
    "license": "MIT",
    "devDependencies": {
        "@types/node": "^24.10.0",
        "typescript": "^5.6.3",
        "vite": "^5.0.0",
        "vue": "^3.5.0"
    },
    "peerDependencies": {
        "vue": "^3.0.0"
    }
}
